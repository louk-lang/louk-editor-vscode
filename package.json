{
  "version": "1.0.0",
  "description": "Syntax highlighting for Louk language",
  "license": "MIT",
  "keywords": [
    "vue"
  ],
  "author": "Alex Gorischek",
  "homepage": "https://louk-lang.org",
  "name": "louk",
  "repository": {
    "type": "git",
    "url": "https://github.com/louk-lang/louk-editor-vscode"
  },
  "displayName": "Louk",
  "icon": "assets/logo.png",
  "publisher": "louk-lang",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "louk",
        "aliases": [
          "Louk"
        ],
        "extensions": [
          ".louk"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "louk",
        "scopeName": "text.html.vue.louk",
        "path": "./syntaxes/louk.tmLanguage.json"
      }
    ]
  }
}